		</rule>
	</rulegroup>

	<rulegroup id="FAST">
		<rule>
			<pattern lang="en">fast</pattern>
			<translation lang="de">schnell</translation>
		</rule>
		<rule>
			<pattern lang="de">fast</pattern>
			<translation lang="en">almost</translation>
