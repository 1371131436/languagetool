		</rule>
	</rulegroup>

	<rulegroup id="HANDY">
		<rule>
			<pattern lang="en">handy</pattern>
			<translation lang="de">praktisch</translation>
			<translation lang="de">handlich</translation>
		</rule>
		<rule>
			<pattern lang="de">Handy</pattern>
			<translation lang="en">cell phone</translation>
			<translation lang="en">mobile phone</translation>
		</rule>
	</rulegroup>

	<rulegroup id="HIGH_SCHOOL">
		<rule>
			<pattern lang="en">high school</pattern>
			<translation lang="de">Gesamtschule</translation>
			<translation lang="de">Gymnasium</translation>
		</rule>
		<rule>
			<pattern lang="de">Hochschule</pattern>
			<translation lang="en">university</translation>
			<translation lang="en">college</translation>
		</rule>
	</rulegroup>

	<rulegroup id="CONFESSION">
		<rule>
			<pattern lang="en">confession</pattern>
			<translation lang="de">Geständnis</translation>
			<translation lang="de">Beichte</translation>
		</rule>
		<rule>
			<pattern lang="de">Konfession</pattern>
			<translation lang="en">religious denomination</translation>
		</rule>
	</rulegroup>

	<rulegroup id="LABOR">
		<rule>
			<pattern lang="en">labor</pattern>
			<translation lang="de">Arbeit</translation>
		</rule>
		<rule>
			<pattern lang="de">Labor</pattern>
			<translation lang="en">laboratory</translation>
		</rule>
	</rulegroup>

	<rulegroup id="LAST_NIGHT">
		<rule>
			<pattern lang="en">last night</pattern>
			<translation lang="de">gestern Abend</translation>
		</rule>
		<rule>
			<pattern lang="de">letzte Nacht</pattern>
			<translation lang="en">during the last night</translation>
		</rule>
	</rulegroup>

	<rulegroup id="LIST">
		<rule>
			<pattern lang="en">list</pattern>
			<translation lang="de">Liste</translation>
			<translation lang="de">Verzeichnis</translation>
		</rule>
		<rule>
			<pattern lang="de">List</pattern>
			<translation lang="en">cunning</translation>
			<translation lang="en">trick</translation>
		</rule>
	</rulegroup>

	<!-- TODO: maybe this belongs to words.xml -->
	<rulegroup id="LIVE">
		<rule>
			<pattern lang="de">live</pattern>
			<translation lang="en">only in the sense of a "live" performance</translation>
		</rule>
	</rulegroup>

	<rulegroup id="LOCAL">
		<rule>
			<pattern lang="en">local</pattern>
