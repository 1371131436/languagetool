<rules>

	<rulegroup id="ALSO">
		<rule>
			<pattern lang="en">also</pattern>
			<translation lang="de">auch</translation>
			<example type="correct">Did he also work at that company?</example>
		</rule>
		<rule>
			<pattern lang="de">also</pattern>
			<translation lang="en">thus</translation>
			<translation lang="en">therefore</translation>
			<translation lang="en">so</translation>
			<example type="correct">Also haben wir uns für die Alternative entschieden.</example>
		</rule>
	</rulegroup>

	<rulegroup id="ACTUAL">
		<rule>
			<pattern lang="en">actual|actually</pattern>
			<translation lang="de">eigentlich</translation>
		</rule>
		<rule>
			<pattern lang="de">aktuell</pattern>
			<translation lang="en">current</translation>
			<translation lang="en">(the) latest</translation>
			<translation lang="en">up-to-date</translation>
		</rule>
	</rulegroup>

	<rulegroup id="ADVOCATE">
		<rule>
			<pattern lang="en">advocate</pattern>
			<translation lang="de">Befürworter</translation>
		</rule>
		<rule>
			<pattern lang="de">Advokat</pattern>
			<translation lang="en">attorney</translation>
			<translation lang="en">lawyer</translation>
		</rule>
	</rulegroup>

	<rulegroup id="ARGUMENT">
		<rule>
			<pattern lang="en">argument</pattern>
			<translation lang="de">Streit</translation>
		</rule>
		<rule>
			<pattern lang="de">Argument</pattern>
			<translation lang="en">point</translation>
			<translation lang="en">reasoned argument</translation>
		</rule>
	</rulegroup>

	<rulegroup id="ART">
		<rule>
			<pattern lang="en">art</pattern>
			<translation lang="de">Kunst</translation>
		</rule>
		<rule>
			<pattern lang="de">Art</pattern>
			<translation lang="en">kind</translation>
			<translation lang="en">manner</translation>
			<translation lang="en">way</translation>
			<translation lang="en">type</translation>
		</rule>
	</rulegroup>

	<rulegroup id="BALD">
		<rule>
			<pattern lang="en">bald</pattern>
			<translation lang="de">glatzköpfig</translation>
			<translation lang="de">kahl</translation>
		</rule>
		<rule>
			<pattern lang="de">bald</pattern>
			<translation lang="en">soon</translation>
		</rule>
	</rulegroup>

	<rulegroup id="BECOME">
		<rule>
			<pattern lang="en">become</pattern>
			<translation lang="de">werden</translation>
		</rule>
		<rule>
			<pattern lang="de">bekommen</pattern>
			<translation lang="en">to get</translation>
			<translation lang="en">receive</translation>
		</rule>
	</rulegroup>

	<rulegroup id="BILLION">
		<rule>
			<pattern lang="en">billion</pattern>
			<translation lang="de">Billion (BE 'billion')</translation>
			<translation lang="de">Milliarde (US 'billion')</translation>
		</rule>
		<rule>
			<pattern lang="de">Billion</pattern>
			<translation lang="en">trillion (US)</translation>
			<translation lang="en">billion (BE)</translation>
		</rule>
	</rulegroup>

	<rulegroup id="BLAME">
		<rule>
			<pattern lang="en">blame</pattern>
			<translation lang="de">Schuld</translation>
			<translation lang="de">jemandem die Schuld zuweisen</translation>
		</rule>
		<rule>
			<pattern lang="de">blamieren</pattern>
			<translation lang="en">embarrass</translation>
			<translation lang="en">make a fool of oneself</translation>
		</rule>
	</rulegroup>

	<rulegroup id="BOX">
		<rule>
			<pattern lang="en">box</pattern>
			<translation lang="de">Kasten</translation>
		</rule>
		<rule>
			<pattern lang="de">Box</pattern>	<!-- TODO: Lautsprecher -->
			<translation lang="en">speaker (stereo system)</translation>
		</rule>
	</rulegroup>

	<rulegroup id="BRAVE">
		<rule>
			<pattern lang="en">brave</pattern>
			<translation lang="de">mutig</translation>
			<translation lang="de">tapfer</translation>
		</rule>
		<rule>
			<pattern lang="de">brav</pattern>
			<translation lang="en">well-behaved</translation>
		</rule>
	</rulegroup>

	<rulegroup id="CHEF">
		<rule>
			<pattern lang="en">chef</pattern>
			<translation lang="de">Chefkoch</translation>
			<translation lang="de">Koch</translation>
		</rule>
		<rule>
			<pattern lang="de">Chef</pattern>
			<translation lang="en">boss</translation>
			<translation lang="en">chief</translation>
		</rule>
	</rulegroup>

	<rulegroup id="CONQUER">
		<rule>
			<pattern lang="en">conquer</pattern>
			<translation lang="de">erobern</translation>
		</rule>
		<rule>
			<pattern lang="en">concur</pattern>
			<translation lang="de">übereinstimmen</translation>
		</rule>
		<rule>
			<pattern lang="de">konkurrieren</pattern>
			<translation lang="en">compete</translation>
		</rule>
	</rulegroup>

	<rulegroup id="CONSEQUENT">
		<rule>
			<pattern lang="en">consequent|consequently</pattern>
			<translation lang="de">folglich</translation>
		</rule>
		<rule>
			<pattern lang="de">konsequent</pattern>
			<translation lang="en">consistent</translation>
		</rule>
	</rulegroup>

	<rulegroup id="CURIOUS">
		<rule>
			<pattern lang="en">curious</pattern>
			<translation lang="de">neugierig</translation>
		</rule>
		<rule>
			<pattern lang="de">kurios</pattern>
			<translation lang="en">odd</translation>
			<translation lang="en">queer</translation>
		</rule>
	</rulegroup>

	<rulegroup id="DECENT">
		<rule>
			<pattern lang="en">decent</pattern>
			<translation lang="de">anständig</translation>
			<translation lang="de">ordentlich</translation>
		</rule>
		<rule>
			<pattern lang="de">dezent</pattern>
			<translation lang="en">discreet(ly)</translation>
			<translation lang="en">unobtrusive</translation>
		</rule>
	</rulegroup>

	<rulegroup id="DOME">
		<rule>
			<pattern lang="en">dome</pattern>
			<translation lang="de">Kuppel</translation>
		</rule>
		<rule>
			<pattern lang="de">Dom</pattern>
			<translation lang="en">cathedral</translation>
		</rule>
	</rulegroup>

	<rulegroup id="ENGAGEMENT">
		<rule>
			<pattern lang="en">engagement</pattern>
			<translation lang="de">Verpflichtung</translation>
			<translation lang="de">Beschäftigung</translation>
			<translation lang="de">Verlobung</translation>
		</rule>
		<rule>
			<pattern lang="de">Engagement</pattern>
			<translation lang="en">motivation</translation>
			<translation lang="en">commitment</translation>
			<translation lang="en">engagement</translation>	<!-- TODO: theater, film -->
		</rule>
	</rulegroup>

	<rulegroup id="ETIQUETTE">
		<rule>
			<pattern lang="en">etiquette</pattern>
			<translation lang="de">Etikette</translation>
		</rule>
		<rule>
			<pattern lang="de">Etikett</pattern>
			<translation lang="en">label</translation>
		</rule>
	</rulegroup>

	<rulegroup id="EVENTUALLY">
		<rule>
			<pattern lang="en">eventually</pattern>
			<translation lang="de">schließlich</translation>
			<translation lang="de">endlich</translation>
		</rule>
		<rule>
			<pattern lang="de">eventuell</pattern>
			<translation lang="en">possibly</translation>
		</rule>
	</rulegroup>

	<rulegroup id="EVERGREEN">
		<rule>
			<pattern lang="en">evergreen</pattern>
			<translation lang="de">immergrüne Pflanze</translation>
		</rule>
		<rule>
			<pattern lang="de">Evergreen</pattern>
			<translation lang="en">classic song</translation>
		</rule>
	</rulegroup>

	<rulegroup id="EXCEPTIONAL">
		<rule>
			<pattern lang="en">exceptional|exceptionally</pattern>
			<translation lang="de">außergewöhnlich</translation>
		</rule>
		<rule>
			<pattern lang="de">ausnahmsweise</pattern>
			<translation lang="en">as an exception</translation>
		</rule>
	</rulegroup>

	<rulegroup id="FAMILIAR">
		<rule>
			<pattern lang="en">familiar</pattern>
