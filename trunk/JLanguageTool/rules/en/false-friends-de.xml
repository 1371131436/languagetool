		</rule>
	</rulegroup>

	<rulegroup id="MARMALADE">
		<rule>
			<pattern lang="en">marmalade</pattern>
			<translation lang="de">Orangenmarmelade</translation>
		</rule>
		<rule>
			<pattern lang="de">Marmelade</pattern>
			<translation lang="en">jam</translation>
		</rule>
	</rulegroup>

	<rulegroup id="MEANING">
		<rule>
			<pattern lang="en">meaning</pattern>
			<translation lang="de">Bedeutung</translation>
		</rule>
		<rule>
			<pattern lang="de">Meinung</pattern>
			<translation lang="en">opinion</translation>
		</rule>
	</rulegroup>

	<rulegroup id="MENU">
		<rule>
