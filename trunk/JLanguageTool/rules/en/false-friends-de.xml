		</rule>
		<rule>
			<pattern lang="de">Notiz</pattern>
			<translation lang="en">written note</translation>
			<translation lang="en">written notes</translation>
			<translation lang="en">brief report</translation>
		</rule>
	</rulegroup>

	<rulegroup id="OBJECTIVE">
		<rule>
			<pattern lang="en">objective</pattern>
