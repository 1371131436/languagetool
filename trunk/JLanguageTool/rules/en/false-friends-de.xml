			<translation lang="de">Speisekarte</translation>
		</rule>
		<rule>
			<pattern lang="de">Menü</pattern>
			<translation lang="en">daily special (restaurant)</translation>
		</rule>
	</rulegroup>

	<rulegroup id="MURDER">
		<rule>
			<pattern lang="en">murder</pattern>
			<translation lang="de">Mord</translation>
		</rule>
		<rule>
			<pattern lang="de">Mörder</pattern>
			<translation lang="en">murderer</translation>
		</rule>
	</rulegroup>

	<rulegroup id="MIST">
