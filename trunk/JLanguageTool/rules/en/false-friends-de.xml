		</rule>
	</rulegroup>

	<rulegroup id="GIFT">
		<rule>
			<pattern lang="en">gift</pattern>
			<translation lang="de">Geschenk</translation>
		</rule>
		<rule>
			<pattern lang="de">Gift</pattern>
			<translation lang="en">poison</translation>
		</rule>
	</rulegroup>

	<rulegroup id="GROSS">
		<rule>
			<pattern lang="en">gross</pattern>
			<translation lang="de">ekelhaft</translation>
			<translation lang="de">grob</translation>
			<translation lang="de">Brutto-</translation>
		</rule>
		<rule>
			<pattern lang="de">groß</pattern>
			<translation lang="en">big</translation>
			<translation lang="en">large</translation>
		</rule>
	</rulegroup>

	<rulegroup id="GYM">
		<rule>
			<pattern lang="en">gym</pattern>	<!-- TODO: gymnasium -->
