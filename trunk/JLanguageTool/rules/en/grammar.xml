<rules>

	<rule id="DET_NOUN" name="fake description">
		<pattern lang="en">"is" "simple"</pattern>
		<message>foo message <em>the</em>!</message>
		<error_rate warnings="117" sentences="75900" />
		<example type="correct"><em>The</em> baseball teams were successful.</example>
		<example type="incorrect"><em>A</em> baseball teams were successful.</example>
	</rule>


