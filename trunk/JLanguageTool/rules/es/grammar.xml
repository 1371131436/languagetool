<rules lang="es">

  <category name="misc">

    <!-- from Spanish Wikipedia -->
    <rule id="DE_GRATIS" name="«de gratis» (gratis)">
      <pattern mark_from="0" case_sensitive="no">
        <token>de</token>
        <token>gratis</token>
      </pattern>
      <message>
        La forma correcta es decir <suggestion>gratis</suggestion>.</message>
      <example type="correct">El agua es gratis.</example>
      <example type="incorrect">El agua es <marker>de gratis</marker>.</example>
    </rule>

  </category>

</rules>
