<rules lang="en">
	
	<rule name="Modal verb + verb/noun -> modal verb + verb" id="MD VB.*">
		<pattern mark="2">
			<token postag="MD"/>
			<and>
			<token postag="VB"/>
			<token postag="NN"/>
			</and>
		</pattern>
		<disambig postag="VB"/>
	</rule>

</rules>