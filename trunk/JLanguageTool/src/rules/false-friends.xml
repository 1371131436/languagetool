		</rule>
		<rule>
			<pattern lang="de"><token>Etikett</token></pattern>
			<translation lang="en">label</translation>
		</rule>
		<rule>
			<pattern lang="pl"><token inflected="yes">etykietka</token></pattern>
			<translation lang="en">label</translation>
		</rule>
		<rule>
			<pattern lang="pl"><token inflected="yes">etykieta</token></pattern>
