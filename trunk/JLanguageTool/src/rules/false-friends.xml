<rules>

	<rulegroup id="ABSURD">
	<rule>
	<pattern lang="en"><token>an</token><token>absurd</token></pattern>
	<translation lang="pl">absurdalny</translation>
	<example type="correct">This is an absurd suggestion!</example>
	</rule>
	<rule>
	<pattern lang="pl"><token>absurd</token></pattern>
	<translation lang="en">absurdity</translation>
	<example type="correct">To absurd!</example>
	</rule>
	</rulegroup>
	
	<rulegroup id="ALSO">
		<rule>
			<pattern lang="en"><token>also</token></pattern>
			<translation lang="de">auch</translation>
			<example type="correct">Did he also work at that company?</example>
		</rule>
		<rule>
			<pattern lang="de"><token>also</token></pattern>
			<translation lang="en">thus</translation>
			<translation lang="en">therefore</translation>
			<translation lang="en">so</translation>
			<example type="correct">Also haben wir uns für die Alternative entschieden.</example>
		</rule>
	</rulegroup>

	<rulegroup id="ACTUAL">
		<rule>
			<pattern lang="en"><token regexp="yes">actual|actually</token></pattern>
			<translation lang="de">eigentlich</translation>
			<translation lang="pl">rzeczywisty</translation>
			<translation lang="fr">vrai</translation>
			<translation lang="fr">effectif</translation>
		</rule>
		<rule>
			<pattern lang="de"><token>aktuell</token></pattern>
			<translation lang="en">current</translation>
			<translation lang="en">(the) latest</translation>
			<translation lang="en">up-to-date</translation>
		</rule>		
		<rule default="off">
			<pattern lang="pl"><token inflected="yes">aktualny</token></pattern>
			<translation lang="en">current</translation>
			<translation lang="en">(the) latest</translation>
			<translation lang="en">up-to-date</translation>
		</rule>		
	</rulegroup>
	
	<rulegroup id="ACTUALIZED">
		<rule>
			<pattern lang="en"><token regexp="yes">actualised|actualized</token></pattern>
			<translation lang="de">verwirklicht</translation>
			<translation lang="pl">urzeczywistniony</translation>
			<translation lang="pl">urzeczywistniony</translation>
		</rule>
		<rule>
			<pattern lang="de"><token>aktualisiert</token></pattern>
			<translation lang="en">updated</translation>
		</rule>		
		<rule>
			<pattern lang="pl"><token inflected="yes" postag_regexp="yes" postag="ppas.*">zaktualizować</token></pattern>
			<translation lang="en">update</translation>
		</rule>				
	</rulegroup>

	<rulegroup id="ACTUALIZE">
		<rule>
			<pattern lang="en"><token regexp="yes">actualise|actualize</token></pattern>
			<translation lang="de">verwirklichen</translation>
			<translation lang="de">realistisch darstellen</translation>
		</rule>
		<rule>
			<pattern lang="de"><token>aktualisieren</token></pattern>
			<translation lang="en">update</translation>
		</rule>		
	</rulegroup>
	
	<rulegroup id="ADEPT">
		<rule>
			<pattern lang="en"><token>adept</token></pattern>
			<translation lang="pl">biegły</translation>
			<translation lang="pl">mistrz</translation>			
			<translation lang="fr">expert</translation>
			<translation lang="es">experto</translation>
			<translation lang="es">hábil</translation>
		</rule>
		<rule>
			<pattern lang="pl"><token inflected="yes">adept</token></pattern>
			<translation lang="en">follower</translation>
			<translation lang="en">admirer</translation>
		</rule>
		<rule>
			<pattern lang="fr"><token inflected="yes">adepte</token></pattern>
			<translation lang="en">follower</translation>
			<translation lang="en">admirer</translation>
		</rule>		
		<rule>
			<pattern lang="es"><token inflected="yes">adepto</token></pattern>
			<translation lang="en">follower</translation>
			<translation lang="en">admirer</translation>
		</rule>		
	</rulegroup>

	<rulegroup id="ADVOCATE">
